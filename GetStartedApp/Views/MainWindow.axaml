<Window x:Class="GetStartedApp.Views.MainWindow"
		xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:core="clr-namespace:GetStartedApp"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:prism="http://prismlibrary.com/"
		xmlns:views="clr-namespace:GetStartedApp.Views"
		xmlns:vm="using:GetStartedApp.ViewModels"
		xmlns:controls="clr-namespace:Avalonia.Controls;assembly=Avalonia.Controls"
		Title="佰奥产线管理系统"
		d:DesignHeight="450"
		d:DesignWidth="800"
		prism:ViewModelLocator.AutoWireViewModel="True"
		x:CompileBindings="True"
		x:DataType="vm:MainWindowViewModel"
		Background="White"
		ExtendClientAreaToDecorationsHint="False"
		Icon="/Assets/avalonia-logo.ico"
		WindowStartupLocation="CenterScreen"
		WindowState="Maximized"
		mc:Ignorable="d">

	<!--  We give a name to the root grid in order to access it later  -->
  <controls:TransitioningContentControl x:Name="ContentContainer" 
                                          >
        <!-- 这里会动态加载不同的页面 -->
    </controls:TransitioningContentControl>
</Window>
